		<section>
			<h2>5. Forms</h2>
			<form action="#" method="get" accept-charset="utf-8">
				<fieldset>
					<legend>Text</legend>
					<input type="text" placeholder="Text">
					<input type="search" placeholder="Search">
					<input type="tel" placeholder="(555) 555-5555">
					<input type="url" placeholder="domain.com">
					<input type="email" placeholder="you@domain.com">
					<input type="password" placeholder="Password">
				</fieldset>
				<fieldset>
					<legend>Textarea</legend>
					<textarea></textarea>
				</fieldset>
				<fieldset>
					<legend>Disabled</legend>
					<input type="text" placeholder="Disabled" disabled>
					<textarea disabled></textarea>
					<select disabled>
						<option>Option 1</option>
						<option>Option 2</option>
						<option>Option 3</option>
					</select>
				</fieldset>
				<fieldset>
					<legend>Date &amp; Time</legend>
					<time datetime=""></time>
					<input type="datetime" placeholder="2012-01-15T13:49:52-05:00" value="2012-01-15T13:49:52-05:00">
					<input type="date" placeholder="January 15th, 2012" value="2012-01-15">
					<input type="month" placeholder="January 2012" value="2012-01">
					<input type="week" placeholder="3" value="2012-W3">
					<input type="time" placeholder="1:49pm" value="13:49:52-05:00">
					<input type="datetime-local">
				</fieldset>
				<fieldset>
					<legend>Numbers and Ranges</legend>
					<input type="number" placeholder="0">
					<input type="range" min="0" max="100">
				</fieldset>
				<fieldset>
					<legend>Other</legend>
					<input type="color" placeholder="#000000">
				</fieldset>
				<fieldset>
					<legend>Options</legend>
					<input type="checkbox">
					<input type="checkbox" checked>
					<input type="radio">
					<input type="radio" checked>
					<select>
						<option>Option 1</option>
						<option>Option 2</option>
						<option>Option 3</option>
					</select>
					<select multiple>
						<option>Option 1</option>
						<option>Option 2</option>
						<option>Option 3</option>
					</select>
				</fieldset>
				<fieldset>
